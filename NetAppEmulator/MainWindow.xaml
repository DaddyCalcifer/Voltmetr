        <Button Content="Задать" HorizontalAlignment="Center" Margin="0,156,0,0" VerticalAlignment="Top" Height="31" Width="108" Click="Button_Click"/>
        <Label Content="V" HorizontalAlignment="Left" Margin="202,55,0,0" VerticalAlignment="Top" FontSize="36"/>

    </Grid>
</Window>
